import IconMySQL from "./IconMySQL.astro";

type JSONValue =
    | string
    | number
    | boolean
    | JSONObject
    | any

interface JSONObject {
  [x: string]: JSONValue;
}
export const ICONS = {
  python: IconPython,
  fastapi: IconFastAPI,
  django: IconDjango,
  flask: IconFlask,
  postgresql: IconPostgreSQL,
  firebase: IconFirebase,
  mongodb: IconMongodb,
  neo4j: IconNeo4j,
  rabbitmq: IconRabbitMQ,
  redis: IconRedis,
  selenium: IconSelenium,
  docker: IconDocker,
  kube: IconKube,
  aws: IconAWS,
  gcp: IconGCP,
  git: IconGit,
  github: IconGitHub,
  javaScript: IconJavaScript,
  typeScript: IconTypeScript,
  nodejs: IconNodeJs,
  astro: IconAstro,
  html: IconHtml,
  css: IconCss,
  trello: IconTrello,
  jira: IconJira,
  oauth: IconOAuth,
  travisci: IconTravisCI,
  mysql: IconMySQL,
  // add new icons here <--
  // Make sure that when calling the icon the key is the same
  // "optional" in the IconKeys const file adds the icon key for when you call IconKeys.nameIcon
};

export function getIconComponent(iconName: string) {
  return ICONS[iconName as keyof typeof ICONS] || null;
}
